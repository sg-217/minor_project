    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mindee": "^4.32.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.7.0",
    "sharp": "^0.32.6",
